<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite thread-count="3" name="Lamba_Test_Suite" parallel="tests">
  
  <test name="FirefoxTest">
  <parameter name="browser" value="firefox"/>
  <parameter name="version" value="82.0"/>
  <parameter name="platform" value="Windows 7"/>
    <classes>
      <class name="Selenium.LambdaTest.Lambda_Automation_Selenium"/>
    </classes>
  </test> <!-- Test -->
  
  <test name="ChromeTest">
  <parameter name="browser" value="chrome"/>
  <parameter name="version" value="88.0"/>
  <parameter name="platform" value="Windows 10"/>
    <classes>
      <class name="Selenium.LambdaTest.Lambda_Automation_Selenium"/>
    </classes>
  </test> <!-- Test -->
  
    <test name="EdgeTest">
  <parameter name="browser" value="edge"/>
  <parameter name="version" value="87.0"/>
  <parameter name="platform" value="macOS Sierra"/>
    <classes>
      <class name="Selenium.LambdaTest.Lambda_Automation_Selenium"/>
    </classes>
  </test> <!-- Test -->
  
   <test name="Internet Explorer">
  <parameter name="browser" value="Internet Explorer"/>
  <parameter name="version" value="11.0"/>
  <parameter name="platform" value="Windows 10"/>
    <classes>
      <class name="Selenium.LambdaTest.Lambda_Automation_Selenium"/>
    </classes>
  </test> <!-- Test -->
  
</suite> <!-- Suite -->